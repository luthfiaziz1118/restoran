<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.syanz.restoran">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconrm"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconrm"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".menu" />
        <activity android:name=".details" />
        <activity android:name=".ListMenu" />
        <activity android:name=".DetailMenuActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".TambahMenu" />
        <activity android:name=".EditData"></activity>
    </application>

</manifest>